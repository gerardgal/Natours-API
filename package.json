{
  "name": "natour",
  "version": "1.0.0",
  "description": "A RESTful API and a server side web app built with: NodeJS, Express, MongoDB and Pug templates.",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "main": "./server.js",
    "dev": "set NODE_ENV=development && nodemon server.js",
    "start:prod": "set NODE_ENV=production && nodemon server.js",
    "import": "node dev-data/data/import-dev-data.js -i",
    "delete": "node dev-data/data/import-dev-data.js -d",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Gerardo Galicia",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "html-to-text": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.1",
    "nodemon": "^2.0.15",
    "pug": "^3.0.2",
    "sharp": "^0.29.3",
    "slugify": "^1.5.3",
    "stripe": "^7.0.0",
    "validator": "^13.6.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.5"
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  }
}
